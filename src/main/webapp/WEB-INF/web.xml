<web-app xmlns="http://java.sun.com/xml/ns/javaee" version="2.5">	<servlet>		<servlet-name>Faces Servlet</servlet-name>		<servlet-class>javax.faces.webapp.FacesServlet</servlet-class>		<load-on-startup>1</load-on-startup>	</servlet>	<servlet-mapping>		<servlet-name>Faces Servlet</servlet-name>		<url-pattern>*.jsf</url-pattern>	</servlet-mapping>	<welcome-file-list>		<welcome-file>index.jsf</welcome-file>	</welcome-file-list></web-app>